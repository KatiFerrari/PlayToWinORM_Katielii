<h1>Cart√µes Cadastrados</h1>

<a href="/cartoes/novo">Cadastre-se aqui!</a>

<table border="1">
    <tr>
        <th>Id</th>
        <th>Nickname</th>
        <th>Nome</th>
    </tr>
    {{#each usuarios}}
    <tr>
        <th>{{this.id}}</th>
        <th>{{this.nickname}}</th>
        <th>{{this.nome}}</th>
        <td>
            <a href="/usuarios/{{this.id}}/update">Atualizar</a>
            <form action="/usuarios/{{this.id}}/delete" method="post">
            <input type="submit" value="Excluir"/>
            </form>
            <a href="/usuarios/{{this.id}}/">
        </td>
    </tr>
    {{/each}}
</table>